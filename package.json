{
  "name": "@typed/fx",
  "version": "0.0.8",
  "description": "High-performance Asynchronous Effects",
  "type": "module",
  "main": "esm/index.js",
  "types": "esm/index.d.ts",
  "typings": "esm/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build:cjs": "rm -rf cjs && tsc -p ./tsconfig.build.cjs.json",
    "build:esm": "rm -rf esm && tsc -p ./tsconfig.build.esm.json",
    "build:package-exports": "tsx ./tools/generatePackageExports.ts",
    "build": "concurrently npm:build:esm npm:build:cjs && npm run build:package-exports",
    "eslint": "NODE_OPTIONS=--max-old-space-size=4096 eslint --ext .ts,.js,.json,.yaml --fix --cache src",
    "mocha": "mocha 'src/**/*.test.ts'",
    "coverage": "c8 --include 'src/**/*' --all npm run mocha",
    "typecheck": "NODE_OPTIONS=--max-old-space-size=4096 tsc --noEmit -p ./tsconfig.build.esm.json",
    "test": "concurrently npm:eslint npm:mocha npm:typecheck",
    "preversion": "npm test",
    "postversion": "npm run build && npm publish --access=public",
    "prepare": "ts-patch install -s",
    "perf:stream:filter-map-reduce": "node --loader @esbuild-kit/esm-loader perf/filter-map-reduce.ts",
    "perf:stream:flatMap": "node --loader @esbuild-kit/esm-loader perf/flatMap.ts",
    "perf:stream:switchMap": "node --loader @esbuild-kit/esm-loader perf/switchMap.ts"
  },
  "keywords": [
    "Fx",
    "Stream",
    "Async",
    "Dependency Injection",
    "hkt-ts"
  ],
  "author": "Tylor Steinberger (tlsteinberger167@gmail.com)",
  "license": "MIT",
  "dependencies": {
    "@effect/core": "^0.0.16",
    "@fp-ts/core": "^0.0.5",
    "@fp-ts/data": "^0.0.3",
    "@tsplus/stdlib": "^0.2.8"
  },
  "peerDependencies": {
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "optionalPeerDependencies": {},
  "devDependencies": {
    "@esbuild-kit/esm-loader": "^2.4.2",
    "@most/core": "^1.6.1",
    "@most/scheduler": "^1.3.0",
    "@most/types": "^1.1.0",
    "@types/benchmark": "^2.1.2",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.45",
    "@types/prettier": "^2.7.0",
    "@typescript-eslint/eslint-plugin": "^5.34.0",
    "@typescript-eslint/parser": "^5.34.0",
    "@zoltu/typescript-transformer-append-js-extension": "^1.0.1",
    "benchmark": "^2.1.4",
    "c8": "^7.12.0",
    "concurrently": "^7.3.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-custom-alias": "^1.3.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fast-check": "^3.1.1",
    "fast-glob": "^3.2.11",
    "husky": "^8.0.1",
    "lint-staged": "^12.5.0",
    "mocha": "^10.0.0",
    "prettier": "^2.7.1",
    "rxjs": "^7.5.7",
    "source-map-support": "^0.5.21",
    "ts-patch": "^2.0.2",
    "tsconfig-paths": "^4.1.0",
    "tsconfig-replace-paths": "^0.0.11",
    "tslib": "^2.4.0",
    "tsx": "^3.8.2",
    "typescript": "^4.7.4",
    "typescript-transform-paths": "^3.3.1"
  },
  "exports": {
    ".": {
      "require": {
        "default": "./cjs/index.js",
        "types": "./cjs/index.d.ts"
      },
      "import": {
        "default": "./esm/index.js",
        "types": "./esm/index.d.ts"
      }
    },
    "./Sink": {
      "require": {
        "default": "./cjs/Sink.js",
        "types": "./cjs/Sink.d.ts"
      },
      "import": {
        "default": "./esm/Sink.js",
        "types": "./esm/Sink.d.ts"
      }
    }
  }
}
